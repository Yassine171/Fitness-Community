                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_marginRight="5sp"
                    android:layout_marginBottom="5sp"
                    app:cardBackgroundColor="#D1FAE5"
                    tools:layout_editor_absoluteX="1dp"
                    tools:layout_editor_absoluteY="587dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/imageView_membre"
                            android:layout_width="wrap_content"
                            android:layout_height="100dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/membre_name_text"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@mipmap/logo_profile_foreground"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/membre_name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/inter_medium"
                            android:gravity="center_horizontal|top"
                            android:text="@string/membre_name_1"
                            android:textAppearance="@style/groupe_name"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.69"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="DuplicateIds" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_marginRight="5sp"
                    android:layout_marginBottom="5sp"
                    app:cardBackgroundColor="#D1FAE5"
                    tools:layout_editor_absoluteX="1dp"
                    tools:layout_editor_absoluteY="587dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/imageView_membre"
                            android:layout_width="wrap_content"
                            android:layout_height="100dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/membre_name_text"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@mipmap/logo_profile_foreground"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/membre_name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/inter_medium"
                            android:gravity="center_horizontal|top"
                            android:text="@string/membre_name_1"
                            android:textAppearance="@style/groupe_name"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.69"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="DuplicateIds" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_marginRight="5sp"
                    android:layout_marginBottom="5sp"
                    app:cardBackgroundColor="#D1FAE5"
                    tools:layout_editor_absoluteX="1dp"
                    tools:layout_editor_absoluteY="587dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/imageView_membre"
                            android:layout_width="wrap_content"
                            android:layout_height="100dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/membre_name_text"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@mipmap/logo_profile_foreground"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/membre_name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/inter_medium"
                            android:gravity="center_horizontal|top"
                            android:text="@string/membre_name_1"
                            android:textAppearance="@style/groupe_name"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.69"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="DuplicateIds" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_marginRight="5sp"
                    android:layout_marginBottom="5sp"
                    app:cardBackgroundColor="#D1FAE5"
                    tools:layout_editor_absoluteX="1dp"
                    tools:layout_editor_absoluteY="587dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/imageView_membre"
                            android:layout_width="wrap_content"
                            android:layout_height="100dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/membre_name_text"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@mipmap/logo_profile_foreground"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/membre_name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/inter_medium"
                            android:gravity="center_horizontal|top"
                            android:text="@string/membre_name_1"
                            android:textAppearance="@style/groupe_name"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.69"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="DuplicateIds" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

            </LinearLayout>
        </ScrollView>

        <TextView
            android:id="@+id/textView_media"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|top"
            android:text="@string/media"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="91dp"
            tools:layout_editor_absoluteY="214dp" />


        <TextView
            android:id="@+id/textView_link"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|top"
            android:text="@string/link"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="281dp"
            tools:layout_editor_absoluteY="214dp" />

        <TextView
            android:id="@+id/textView_docs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center_horizontal|top"
            android:text="@string/docs"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="183dp"
            tools:layout_editor_absoluteY="214dp" />

        <ImageView
            android:id="@+id/imageView_media"
            android:layout_width="147dp"
            android:layout_height="135dp"
            app:srcCompat="@mipmap/logo_media_1_foreground"
            tools:layout_editor_absoluteX="40dp"
            tools:layout_editor_absoluteY="103dp" />

        <ImageView
            android:id="@+id/imageView_docs"
            android:layout_width="149dp"
            android:layout_height="133dp"
            app:srcCompat="@mipmap/logo_docs_foreground"
            tools:layout_editor_absoluteX="132dp"
            tools:layout_editor_absoluteY="105dp" />

        <ImageView
            android:id="@+id/imageView_link"
            android:layout_width="135dp"
            android:layout_height="131dp"
            app:srcCompat="@mipmap/logo_link_foreground"
            tools:layout_editor_absoluteX="235dp"
            tools:layout_editor_absoluteY="105dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>